{
  "openapi": "3.0.0",
  "info": {
    "title": "Ideogram V3 Turbo",
    "version": "1.0.0",
    "description": "Turbo is the fastest and cheapest Ideogram v3. v3 creates images with stunning realism, creative designs, and consistent styles. Perfect for logo generation with professional quality."
  },
  "servers": [
    {
      "url": "https://api.replicate.com/v1",
      "description": "Replicate API"
    }
  ],
  "paths": {
    "/predictions": {
      "post": {
        "operationId": "create_prediction",
        "summary": "Create a new image generation prediction",
        "description": "Create a new prediction for generating images using Ideogram V3 Turbo model",
        "requestBody": {
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "required": ["version", "input"],
                "properties": {
                  "version": {
                    "type": "string",
                    "const": "f8a8eb2c75d7d86ec58e3b8309cee63acb437fbab2695bc5004acf64d2de61a7",
                    "description": "Model version ID"
                  },
                  "input": {
                    "$ref": "#/components/schemas/IdeogramInput"
                  },
                  "webhook": {
                    "type": "string",
                    "format": "uri",
                    "description": "Optional webhook URL for completion notification"
                  }
                }
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Prediction created successfully",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PredictionResponse"
                }
              }
            }
          }
        }
      }
    },
    "/predictions/{prediction_id}": {
      "get": {
        "operationId": "get_prediction",
        "summary": "Get prediction status and results",
        "description": "Get the status and results of an image generation prediction",
        "parameters": [
          {
            "name": "prediction_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The prediction ID"
          }
        ],
        "responses": {
          "200": {
            "description": "Prediction details",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PredictionResponse"
                }
              }
            }
          }
        }
      }
    },
    "/predictions/{prediction_id}/cancel": {
      "post": {
        "operationId": "cancel_prediction",
        "summary": "Cancel a running prediction",
        "description": "Cancel a running image generation prediction",
        "parameters": [
          {
            "name": "prediction_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "The prediction ID to cancel"
          }
        ],
        "responses": {
          "200": {
            "description": "Prediction cancelled",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/PredictionResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "IdeogramInput": {
        "type": "object",
        "required": ["prompt"],
        "properties": {
          "prompt": {
            "type": "string",
            "description": "Text prompt for image generation"
          },
          "aspect_ratio": {
            "type": "string",
            "enum": ["1:1", "16:10", "10:16", "16:9", "9:16", "4:3", "3:4", "21:9", "9:21", "3:2", "2:3"],
            "default": "1:1",
            "description": "Aspect ratio for the generated image"
          },
          "resolution": {
            "type": "string",
            "enum": [
              "640x1344", "768x1344", "832x1216", "896x1152", "1024x1024", 
              "1152x896", "1216x832", "1344x768", "1344x640"
            ],
            "description": "Resolution in pixels (optional, inferred from aspect_ratio if not provided)"
          },
          "style_type": {
            "type": "string",
            "enum": ["None", "Auto", "General", "Realistic", "Design", "3D", "Anime"],
            "default": "Auto",
            "description": "Style type for the generated image"
          },
          "magic_prompt_option": {
            "type": "string",
            "enum": ["Auto", "On", "Off"],
            "default": "Auto",
            "description": "Whether to enhance the prompt automatically"
          },
          "seed": {
            "type": "integer",
            "description": "Random seed for reproducible results"
          },
          "image": {
            "type": "string",
            "format": "uri",
            "description": "Input image URL for image-to-image generation"
          },
          "mask": {
            "type": "string",
            "format": "uri", 
            "description": "Mask image URL for inpainting"
          },
          "style_reference_images": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri"
            },
            "description": "Array of reference image URLs for style consistency"
          }
        }
      },
      "PredictionResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "Unique prediction ID"
          },
          "status": {
            "type": "string",
            "enum": ["starting", "processing", "succeeded", "failed", "canceled"],
            "description": "Current status of the prediction"
          },
          "version": {
            "type": "string", 
            "description": "Model version used"
          },
          "input": {
            "$ref": "#/components/schemas/IdeogramInput"
          },
          "output": {
            "type": "array",
            "items": {
              "type": "string",
              "format": "uri"
            },
            "description": "Array of generated image URLs (when succeeded)"
          },
          "error": {
            "type": "string",
            "description": "Error message if prediction failed"
          },
          "logs": {
            "type": "string",
            "description": "Execution logs"
          },
          "created_at": {
            "type": "string",
            "format": "date-time",
            "description": "Creation timestamp"
          },
          "started_at": {
            "type": "string",
            "format": "date-time",
            "description": "Start timestamp"
          },
          "completed_at": {
            "type": "string", 
            "format": "date-time",
            "description": "Completion timestamp"
          },
          "metrics": {
            "type": "object",
            "properties": {
              "predict_time": {
                "type": "number",
                "description": "Prediction time in seconds"
              }
            }
          },
          "urls": {
            "type": "object",
            "properties": {
              "get": {
                "type": "string",
                "format": "uri",
                "description": "URL to get prediction status"
              },
              "cancel": {
                "type": "string",
                "format": "uri", 
                "description": "URL to cancel prediction"
              }
            }
          }
        }
      }
    },
    "securitySchemes": {
      "ApiTokenAuth": {
        "type": "http",
        "scheme": "bearer",
        "description": "Replicate API token"
      }
    }
  },
  "security": [
    {
      "ApiTokenAuth": []
    }
  ],
  "_metadata": {
    "replicateModel": {
      "owner": "ideogram-ai",
      "name": "ideogram-v3-turbo",
      "versionId": "f8a8eb2c75d7d86ec58e3b8309cee63acb437fbab2695bc5004acf64d2de61a7",
      "pricing": "$0.03 per output image",
      "description": "Fastest and cheapest Ideogram v3 model, perfect for logo generation",
      "hardware": "CPU",
      "commercialUse": true
    }
  }
}